type Admin {
  id: ID! @id
  username: String! @unique
  password: String!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type User {
  id: ID! @id
  name: String!
  birthday: DateTime!
  email: String! @unique
  cellPhone: String! @unique
  company: String
  companyCategory: String
  team: String
  position: String
  workPhone: String
  major: Major!
  graduated_year: GradYear!
  photo: Photo!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Prof {
  id: ID! @id
  name: String!
  email: String! @unique
  workPhone: String
  title: String!
  position: String!
  photo: Photo!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Major {
  id: ID! @id
  name: String!
  users: [User!]!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type GradYear {
  id: ID! @id
  year: Int!
  semester: Int!
  generation: Int!
  users: [User!]!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Notice {
  id: ID! @id
  files: [File!]!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Photo {
  id: ID! @id
  url: String!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
